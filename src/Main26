//大数据的处理方法   
 import java.math.BigInteger;
    import java.util.*;

    public class Main {


        public static void main(String[] args) {
            Scanner sc = new Scanner(System.in);
            while(sc.hasNext()){
               int n =sc.nextInt();
               BigInteger sum=BigInteger.ONE;
                for (int i = 1; i <=n ; i++) {
                    sum=(sum.multiply(BigInteger.valueOf(i)));
                }
                System.out.println(sum);

            }
        }
    }













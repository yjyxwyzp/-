import java.util.Scanner;

public class Main2 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        while(sc.hasNext()){
            int n = 12;
            double sum=0;
            while(n-->0){
            sum+=sc.nextDouble();
            }
            System.out.print("$");
            System.out.printf("%.2f",sum/12.0);
            System.out.println();
        }

            }
        }


